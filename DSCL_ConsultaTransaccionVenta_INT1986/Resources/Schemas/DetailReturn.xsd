                          <xs:element type="xs:int" name="Solicitud"/>
                          <xs:element type="xs:int" name="TipoSolicitud"/>
                          <xs:element type="xs:int" name="EstadoSolicitud"/>
                        </xs:sequence>
                      </xs:complexType>
